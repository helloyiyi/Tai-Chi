  }\r\n\r\n    html.innerHTML = string2; //往demo里写字，substring可以从头开始显示\r\n    style.innerHTML = string.substring(0, n);\r\n    window.scrollTo(0, 99999);\r\n    html.scrollTo(0, 99999);\r\n    if (n < string.length - 1) {\r\n      //如果n不是最后一个，就继续\r\n      n += 1;\r\n      step();\r\n    }\r\n  }, 50);\r\n};\r\n\r\nstep();\r\n"},"lineCount":null}},"hash":"652bd7c8bc3675dfde3a5cb432e6cb81","cacheData":{"env":{}}}